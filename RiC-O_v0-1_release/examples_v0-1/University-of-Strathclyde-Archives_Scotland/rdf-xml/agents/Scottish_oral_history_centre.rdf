    <rico:Agent rdf:about="agent/scottish-oral-history-centre">
        <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#CorporateBody"/>
        <rdfs:label xml:lang="en">University of Strathclyde | Scottish Oral History
            Centre</rdfs:label>
        <rico:hasAgentName>
            <rico:AgentName>
                <rdfs:label xml:lang="en">University of Strathclyde | Scottish Oral History
