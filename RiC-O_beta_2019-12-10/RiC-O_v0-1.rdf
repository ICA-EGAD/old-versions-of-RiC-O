                <owl:onClass rdf:resource="https://www.ica.org/standards/RiC/ontology#Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Connects at lest one Person, to at least another Person, when the first has child the second one.</rdfs:comment>
        <rdfs:label xml:lang="en">Child Relation</rdfs:label>
        <RiCCMCorrespondingComponent xml:lang="en">Class implementation of RiC-R018 and RiC-R018i relations</RiCCMCorrespondingComponent>
    </owl:Class>
    


