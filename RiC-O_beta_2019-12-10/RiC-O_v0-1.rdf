        <rdfs:label xml:lang="en">Corporate Body Type</rdfs:label>
        <RiCCMCorrespondingComponent xml:lang="en">Corresponds to RiC-A12 (Corporate Body Type attribute)</RiCCMCorrespondingComponent>
    </owl:Class>
    


    <!-- https://www.ica.org/standards/RiC/ontology#CorrespondenceRelation -->

