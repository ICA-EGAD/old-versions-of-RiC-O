        <rdfs:label xml:lang="en">Title</rdfs:label>
        <RiCCMCorrespondingComponent xml:lang="en">Class implementation of a specialization of RiC-A28 (Name attribute)</RiCCMCorrespondingComponent>
    </owl:Class>
    


