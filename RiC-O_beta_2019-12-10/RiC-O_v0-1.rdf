        <skos:scopeNote xml:lang="en">Would probably rarely be used as such (use its sub-categories)</skos:scopeNote>
        <RiCCMCorrespondingComponent xml:lang="en">Class implementation of RiC-R036 and RiC-R036i relations</RiCCMCorrespondingComponent>
    </owl:Class>
    


    <!-- https://www.ica.org/standards/RiC/ontology#CarrierType -->

    <owl:Class rdf:about="https://www.ica.org/standards/RiC/ontology#CarrierType">
        <rdfs:subClassOf rdf:resource="https://www.ica.org/standards/RiC/ontology#Type"/>
