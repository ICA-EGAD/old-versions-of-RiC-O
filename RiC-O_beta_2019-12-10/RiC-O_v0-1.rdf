                <owl:onClass rdf:resource="https://www.ica.org/standards/RiC/ontology#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Connects at least one Record Resource or Instantiation to at least one Activity, when the Record Resource or Instantiation results from the activity.</rdfs:comment>
        <rdfs:label xml:lang="en">Activity Documentation Relation</rdfs:label>
        <RiCCMCorrespondingComponent xml:lang="en">Class implementation of RiC-R033 and RiC-R033 relations</RiCCMCorrespondingComponent>
    </owl:Class>
    


    <!-- https://www.ica.org/standards/RiC/ontology#ActivityType -->

