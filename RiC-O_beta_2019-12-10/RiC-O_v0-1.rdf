        <rdfs:comment xml:lang="en">Connects at least one Position, and a Group, when the first one(s) exist(s) in/is defined within the second one.</rdfs:comment>
        <rdfs:label xml:lang="en">Position to Group Relation</rdfs:label>
        <RiCCMCorrespondingComponent xml:lang="en">Class implementation of RiC-R056 and RiC-R056i relations</RiCCMCorrespondingComponent>
    </owl:Class>
    


    <!-- https://www.ica.org/standards/RiC/ontology#ProductionTechniqueType -->

    <owl:Class rdf:about="https://www.ica.org/standards/RiC/ontology#ProductionTechniqueType">
